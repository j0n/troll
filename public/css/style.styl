@import 'fonts'
global-reset()
body {
  margin: 0;
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif;
  border-radius: 10px;
  background: #FFF;
}
#stage {
  position: absolute;
  width: 100%;
  height: 100%;
}
#debug {
  display: none;
  background: blue;
}
.block {
  position: absolute;
  width: 50px;
  height: 50px;
  background: green;
  z-index: 2;
}

